{
    "manifest_version": 2,
    "name": "Github Dashboard Remover",
    "description": "Removes the Github Dashboard. Why? Just not to be distracted by people you follow.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://github.com/"],
            "js": ["remove-dashboard.js"]
        },
        {
            "matches": ["https://github.com/tarasowski"],
            "js": ["remove-socialbar.js"]
        }
    ]
}
